{
  "name": "pokedex",
  "version": "1.0.0",
  "description": "Visualize PokeAPI Data",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "build": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url .",
    "watch": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url .",
    "compile:sass": "node-sass public/sass/main.scss public/css/main.css -w"
  },
  "author": "apintok",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "parcel-bundler": "^1.12.4"
  }
}
